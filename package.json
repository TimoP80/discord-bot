{
  "name": "discord-bot",
  "version": "4.1.1",
  "description": "",
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "start": "node start.cjs",
    "start:prod": "NODE_ENV=production node start.cjs",
    "build": "tsc && npm run copy-html",
    "dev": "npm run build && npm run start",
    "test": "node test-agent-platform.js",
    "test:agent": "node test-agent-platform.js",
    "demo:gui": "node demo-gui.js",
    "demo:full-gui": "node demo-full-gui.js",
    "test:finnish": "node test-finnish-llama3-validation.js",
    "setup:finnish": "bash setup-finnish-model.sh",
    "setup:finnish:large": "powershell -ExecutionPolicy Bypass -File setup-large-finnish-model.ps1",
    "test:electron": "npm run build && node test-electron-gui.js",
    "list-models": "ts-node list-models.ts",
    "generate-invite": "ts-node src/generate-invite.ts",
    "deploy-commands": "ts-node src/deploy-commands.ts",
    "regenerate-token": "ts-node src/token-utils.ts",
    "manage-bots": "ts-node src/manage-bots.ts",
    "prod": "npm run build && npm run start:prod",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "set-model": "ts-node set-model.ts",
    "electron": "electron dist/electron-main.js",
    "electron:dev": "npm run build && electron dist/electron-main.js",
    "build:electron": "tsc && npm run copy-html",
    "copy-html": "copyfiles -f src/*.html dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@elevenlabs/client": "^0.12.2",
    "@elevenlabs/elevenlabs-js": "^2.26.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google-cloud/speech": "^7.2.1",
    "@google/genai": "^1.29.1",
    "@google/generative-ai": "^0.24.1",
    "@snazzah/davey": "^0.1.8",
    "@types/fuse": "^2.5.2",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.7.2",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "electron": "^39.2.7",
    "fluent-ffmpeg": "^2.1.3",
    "fuse.js": "^7.1.0",
    "google-tts-api": "^0.0.6",
    "help": "^3.0.2",
    "node-opus": "^0.3.3",
    "open": "^10.1.0",
    "openai": "^6.10.0",
    "prism-media": "^1.3.5",
    "reflect-metadata": "^0.2.2",
    "sodium-native": "^5.0.10",
    "soundcloud-scraper": "^5.0.3",
    "tsc": "^2.0.4",
    "whisper-node": "^1.1.1",
    "yt-search": "^2.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
